<p id="notice"><%= notice %></p>

<h1>練習表</h1>

<table>
  <thead>
    <tr>
      <th>バンド名</th>
      <th>代表者</th>
      <th>日付</th>
      <th>時限</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.description %></td>
        <td><%= event.get_date %></td>
        <td><%= event.define_gen %></td>
        <td><%= link_to '詳細', event %></td>
        <td><%= link_to '編集', edit_event_path(event) %></td>
        <td><%= link_to '削除', event, method: :delete, data: { confirm: '削除しますか？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '練習の登録', new_event_path %>

<div id="calendar"></div>
